#include<stdio.h>
int main()
{
    int units,amt,tamt,charge;
    printf("Enter the units : ");
    scanf("%d",&units);
    if(units<=50)
    {
        amt=units*0.5;
    }
    else if (units<=150)
    {
        amt=units*0.75;
    }
    else if(units<=350)
    {
        amt=units*1.20;
    }
    else
    {
        amt=units*1.50;
    }
    amt=amt*0.2;
    printf("%d",amt);
}
